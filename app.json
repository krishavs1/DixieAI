{
  "expo": {
    "name": "Dixie AI",
    "slug": "dixie-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dixieai.mobile"
    },
    "android": {
      "package": "com.dixieai.mobile",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {},
    "scheme": "dixie-ai",
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-dev-client",
      "expo-web-browser",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.440630945257-d3gbupl3uaafv10sib53r2q6eh4mqpku"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f318ce8e-3c7c-4779-a97f-04e579a3d6eb"
      }
    },
    "owner": "krishavs1"
  }
}
