{
  "expo": {
    "name": "Dixie AI",
    "slug": "dixie-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dixieai.mobile"
    },
    "android": {
      "package": "com.dixieai.mobile",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {},
    "scheme": "dixie-ai",
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      "expo-dev-client"
    ]
  }
}
